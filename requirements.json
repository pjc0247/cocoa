[
  {
	"name" : "python",
    "uri" : "https://www.python.org/ftp/python/2.7.10/python-2.7.10.msi",
    "local" : "python-2.7.10.msi",
    "path" : "c:\\cocoa\\python27",
    "optional_if" : "",
    "env_key" : "",
    "cmd" : "msiexec /i {local} /quiet /log logs.txt TARGETDIR={path}"
  },
  {
	"name" : "jdk",
    "uri" : "http://download.oracle.com/otn-pub/java/jdk/8u45-b15/jdk-8u45-windows-i586.exe",
    "local" : "jdk-8u45-windows-i586.exe",
    "path" : "c:\\cocoa\\jdk",
    "optional_if" : "",
    "env_key" : "JAVA_HOME",
    "cmd" : "{local} /s INSTALLDIR={path} /L logs.txt"
  },
  {
	"name" : "ant",
    "uri" : "http://mirror.apache-kr.org//ant/binaries/apache-ant-1.9.5-bin.zip",
    "local" : "apache-ant-1.9.5-bin.zip",
    "path" : "c:\\cocoa\\ant",
    "optional_if" : "",
    "env_key" : "ANT_ROOT",
    "cmd" : "unzip {local} -d {path}"
  },
  {
	"name" : "ndk",
    "uri" : "http://dl.google.com/android/ndk/android-ndk-r10e-windows-x86.exe",
    "local" : "android-ndk-r10e-windows-x86.exe",
    "path" : "c:\\cocoa\\ndk",
    "optional_if" : "",
    "env_key" : "NDK_ROOT",
    "cmd" : "{local} "
  },
  {
	"name" : "android_sdk",
    "uri" : "http://dl.google.com/android/android-sdk_r24.3.2-windows.zip",
    "local" : "android-sdk_r24.3.2-windows.zip",
    "path" : "c:\\cocoa\\android_sdk",
    "optional_if" : "",
    "env_key" : "ANDROID_SDK_ROOT",
    "cmd" : "unzip {local} -d {path}"
  },
  {
	"name" : "cygwin",
    "uri" : "http://cygwin.com/setup-x86.exe",
    "local" : "setup-x86.exe",
    "path" : "c:\\cocoa\\cygwin",
    "optional_if" : "cocos_v2",
    "env_key" : "",
    "cmd" : "{local} -q -s ftp://ftp.kaist.ac.kr/cygwin/ -p gcc,gcc-core,gcc-g++,python,automake -W -R {path}"
  }
]